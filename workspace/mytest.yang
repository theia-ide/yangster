         leaf augmentLeaf2 {
             type string;
         }
     }

     augment "/myt:testcontainer/myt:innerTestContainer" {
         uses extmod:mytest2Group;
     }

     augment "/extmod:bla" {
         leaf blaLeaf {
             type string;
         }
