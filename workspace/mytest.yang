     container testcontainer {
            container innerTestContainer {
                list innerTestList {
                    key "keyLeaf";
                    container listContainer {
                        leaf meAlone {
                            type string;
                        }
                    }
                    leaf keyLeaf {
                        type string;
                    }
                }
                leaf-list leafList {
                    type string;
                }
                leaf anotherLeaf {
                    type string;
                }
            }
            leaf testleaf {
                type string;
                description "this is a simple test lea";
            }
          
    }

    container groupingTest {
        uses anotherGroup;
        choice really {
            case yes {
                container yesWeCan {
                
                }
            }
            case no {
