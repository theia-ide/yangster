module mytest {
    yang-version 1.1;
    namespace "urn:example:system";
    prefix "myt";

    import mytest2{
        prefix "extmod";
    }
    
    include mytest_sub;
    
    typedef my-type {
       status deprecated;
       type int32;
     }

     identity myid {


































































     
     }

    grouping endpoint {
       description "A reusable endpoint group.";
       leaf ip {
         type string;
       }
       leaf port {
         type string;
       }
     }

     grouping anotherGroup {
         description "Another group, yo!";
         leaf anotherGroupLeaf {
             type string;
         }
     }
     
     augment "/myt:testcontainer" {
         leaf augmentLeaf {
             type string;
         }
